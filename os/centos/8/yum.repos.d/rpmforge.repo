### Name: RPMforge RPM Repository for RHEL 8 - dag
### URL: http://rpmforge.net/
[rpmforge]
name = RHEL $releasever - RPMforge.net - dag
baseurl = http://apt.sw.be/redhat/el8/en/$basearch/rpmforge
mirrorlist = http://mirrorlist.repoforge.org/el8/mirrors-rpmforge
enabled = 0
gpgcheck = 0

[rpmforge-extras]
name = RHEL $releasever - RPMforge.net - extras
baseurl = http://apt.sw.be/redhat/el8/en/$basearch/extras
mirrorlist = http://mirrorlist.repoforge.org/el8/mirrors-rpmforge-extras
enabled = 0
gpgcheck = 0

[rpmforge-testing]
name = RHEL $releasever - RPMforge.net - testing
baseurl = http://apt.sw.be/redhat/el8/en/$basearch/testing
mirrorlist = http://mirrorlist.repoforge.org/el8/mirrors-rpmforge-testing
enabled = 0
gpgcheck = 0

